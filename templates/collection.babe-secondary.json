er('mousemove', (e) => {\n        if(!isDown) return;\n        e.preventDefault();\n        dragged = true;  \/\/ Set dragged flag on mousemove\n        const x = e.pageX - el.getBoundingClientRect().left;  \/\/ Use getBoundingClientRect().left\n        const walk = (x - startX) * 2; \/\/ Multiplied by 3 for faster scrolling\n        el.scrollLeft = scrollLeft - walk;\n    });\n\n    \/\/ Prevent click event if dragged\n    el.addEventListener('click', (e) => {\n        if (dragged) {\n            e.preventDefault();\n            e.stopPropagation();\n        }\n    }, true);  \/\/ Use capturing phase to stop event propagation\n});\n\n<\/script>\n<\/body>\n<\/html>","width":"100%","alignment":"center"}}},"block_order":["706256ac-99ba-47a0-99ff-b1ee91da9229"],"settings":{"full_width":true,"space_around":false}},"main-collection":{"type":"main-collection","blocks":{"collection_description":{"type":"collection_description","settings":{}},"subcollections":{"type":"subcollections","settings":{}},"product_grid":{"type":"product_grid","settings":{"grid_style":"small"}}},"block_order":["collection_description","subcollections","product_grid"],"settings":{"enable_sidebar":false,"collapse_filters":true,"enable_color_swatches":false,"enable_sort":true}},"ea2d96c1-ceb7-49c5-8193-4681f8a19f45":{"type":"featured-collection","settings":{"title":"APPLICATION & REMOVAL","home_featured_products":"babe-hair-tools-applications-and-removals","count":5,"mobile_scrollable":true,"view_all":true,"divider":false}},"f6bbd35a-b428-4744-973d-9e0d0eb85707":{"type":"featured-collection","settings":{"title":"EXTENSION CARE","home_featured_products":"babe-hair-extension-care","count":5,"mobile_scrollable":true,"view_all":true,"divider":false}},"recently-viewed":{"type":"recently-viewed","settings":{}}},"order":["collection-header","a6b7b6d7-0af0-437d-96d3-6ace311b7d93","01a23c2a-601b-42b2-8841-3b7491bf385f","main-collection","ea2d96c1-ceb7-49c5-8193-4681f8a19f45","f6bbd35a-b428-4744-973d-9e0d0eb857
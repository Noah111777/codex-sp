te;\n        opacity: 0;\n        transition: opacity 0.3s ease-in-out;\n    }\n    .tab:hover::before {\n        opacity: 1;\n    }\n    @keyframes shimmer {\n        0% { background-position: -468px 0; }\n        100% { background-position: 468px 0; }\n    }\n    .header-text {\n        text-align: center;\n        font-weight: bold;\n        margin: 20px 0 10px;\n    }\n\n    \/* Scroll arrow *\/\n    .scroll-arrow {\n        position: fixed;\n        right: 10px;\n        width: 30px;\n        height: 30px;\n        background-color: rgba(0,0,0,0.3);\n        border-radius: 50%;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        color: white;\n        font-size: 20px;\n        pointer-events: none;\n        opacity: 0;\n        transition: opacity 0.3s ease;\n        z-index: 1000;\n    }\n\n    \/* Desktop styles *\/\n    @media (min-width: 768px) {\n        .tabs-container {\n            overflow-x: visible;\n        }\n        .tabs {\n            flex-wrap: wrap;\n            justify-content: center;\n        }\n        .tab {\n            margin-bottom: 10px;\n        }\n        .scroll-arrow {\n            display: none;\n        }\n    }\n<\/style>\n<\/head>\n\n<body>\n    <!-- Banner Section -->\n    <a href=\"https:\/\/www.internationalbeauty.ca\/pages\/catalogues\" target=\"_blank\">\n        <div class=\"lpimage-banner\">\n            <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0442\/3516\/1766\/files\/IBSS_july-august_banner_1.webp?v=1722464358\" alt=\"July Banner - Click to view catalogues\">\n        <\/div>\n    <\/a>\n\n    <!-- Buttons Section -->\n    <div class=\"header-text\">\n        Click to go to\n    <\/div>\n    <div class=\"tabs-container\" id=\"tabsContainer\">\n        <div class=\"tabs\">\n            <div class=\"tab\" onclick=\"scrollToElement('shopify-section-template--17214040801446__slideshow_BndRCc')\">Kevin Murphy<\/div>\n            <div class=\"tab\" onclick=\"scrollToEle
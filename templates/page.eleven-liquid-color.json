: absolute;\n    top: 0;\n    bottom: 0;\n    width: 2px;\n    z-index: 2;\n    pointer-events: none;\n}\n\n.scrolling-menu-container::before {\n    left: 0;\n    background: linear-gradient(to right, rgba(255,255,255,1), rgba(255,255,255,0));\n}\n\n.scrolling-menu-container::after {\n    right: 0;\n    background: linear-gradient(to left, rgba(255,255,255,1), rgba(255,255,255,0));\n}\n\n\/* Your existing CSS here *\/\n\n\/* Additional CSS for the H2 header *\/\nh2 {\n    display: flex;\n    align-items: center;\n    justify-content: start; \/* Ensures the text and SVG are left-aligned *\/\n}\n\n\/* Styling for the arrow icon *\/\n.arrow-icon {\n    margin-left: 10px;\n    align-self: center; \/* This will vertically center the SVG with respect to the text *\/\n}\n\n\/* Mobile-specific adjustments *\/\n@media (max-width: 768px) {\n    .arrow-icon {\n        margin-top: -2px; \/* Adjust this value to align the SVG as needed *\/\n    }\n}\n\n    <\/style>\n<\/head>\n<body>\n  <div class=\"scrolling-menu-container\">\n           <h2>\n            Eleven Australia Color\n            <!-- SVG arrow icon -->\n            <svg class=\"arrow-icon\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\">\n                <path d=\"M8.59 16.34l4.58-4.59-4.58-4.59L10 5.75l6 6-6 6z\"\/>\n                <path d=\"M0-.25h24v24H0z\" fill=\"none\"\/>\n            <\/svg>\n        <\/h2>\n       <div class=\"scrolling-menu\" id=\"scrollingMenu\">\n      <a href=\"https:\/\/www.internationalbeauty.ca\/pages\/eleven-australia-liquid-color\">\n          <img loading=\"lazy\" src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0442\/3516\/1766\/files\/Liquid_Color_eleven_australia.webp?v=1699026016\" alt=\"eleven liquid color\" draggable=\"false\">\n      <\/a>\n      <a href=\"https:\/\/www.internationalbeauty.ca\/collections\/eleven-australia-liquid-color-bleach\">\n          <img loading=\"lazy\" src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0442\/3516\/1766\/files\/Lightening_powder_eleven_aus
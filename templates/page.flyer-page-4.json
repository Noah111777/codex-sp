{"sections":{"main":{"type":"main-page","disabled":true,"settings":{}},"advanced_content_Di6tFU":{"type":"advanced-content","blocks":{"liquid_D7XdzH":{"type":"liquid","settings":{"code":"<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\">\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n<title>Banner and Responsive Tabs<\/title>\n<style>\n    \/* Banner Styles *\/\n    .lpimage-banner {\n        width: 100%;\n        overflow: hidden;\n    }\n\n    .lpimage-banner img {\n        width: 100%;\n        height: auto;\n        display: block;\n    }\n\n    \/* Tabs Styles *\/\n    .tabs-container {\n        position: relative;\n        overflow-x: auto;\n        -webkit-overflow-scrolling: touch;\n        scrollbar-width: none; \/* Firefox *\/\n        -ms-overflow-style: none;  \/* Internet Explorer 10+ *\/\n        padding: 10px 0;\n    }\n    .tabs-container::-webkit-scrollbar { \n        display: none;  \/* Safari and Chrome *\/\n    }\n    .tabs {\n        display: flex;\n        white-space: nowrap;\n        padding: 0 10px;\n    }\n    .tab {\n        flex: 0 0 auto;\n        padding: 10px 20px;\n        margin-right: 10px;\n        color: #000000;\n        background-color: #ffffff;\n        border: 2px solid #000000;\n        border-radius: 30px;\n        cursor: pointer;\n        transition: background-color 0.2s ease-in-out;\n        position: relative;\n        overflow: hidden;\n        font-size: 16px;\n        text-decoration: none !important; \/* Force remove underline *\/\n        display: inline-flex;\n        align-items: center;\n        justify-content: center;\n    }\n    .tab::before {\n        content: \"\";\n        position: absolute;\n        top: 0;\n        right: 0;\n        bottom: 0;\n        left: 0;\n        background: linear-gradient(90deg, rgba(0,0,0,.2) 25%, rgba(0,0,0,.3) 50%, rgba(0,0,0,.1) 75%) repeat;\n        background-size: 200% 100%;\n        animation: shimmer 8.8s linear infinite;\n        opacity: 0;\n        transition: opacity 0.3s ease-in-out;\n    }\n    .tab:hover::before {\n        opacity: 1;\n    }\n    @keyframes shimmer {\n        0% { background-position: -468px 0; }\n        100% { background-position: 468px 0; }\n    }\n    .header-text {\n        text-align: center;\n        font-weight: bold;\n        margin: 20px 0 10px;\n    }\n\n    \/* Scroll arrow *\/\n    .scroll-arrow {\n        position: fixed;\n        right: 10px;\n        width: 30px;\n        height: 30px;\n        background-color: rgba(0,0,0,0.3);\n        border-radius: 50%;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        color: white;\n        font-size: 20px;\n        pointer-events: none;\n        opacity: 0;\n        transition: opacity 0.3s ease;\n        z-index: 1000;\n    }\n\n    \/* Desktop styles *\/\n    @media (min-width: 768px) {\n        .tabs-container {\n            overflow-x: visible;\n        }\n        .tabs {\n            flex-wrap: wrap;\n            justify-content: center;\n        }\n        .tab {\n            margin-bottom: 10px;\n        }\n        .scroll-arrow {\n            display: none;\n        }\n    }\n<\/style>\n<\/head>\n\n<body>\n    <!-- Banner Section -->\n    <a href=\"https:\/\/www.internationalbeauty.ca\/pages\/catalogues\" target=\"_blank\">\n        <div class=\"lpimage-banner\">\n            <img src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0442\/3516\/1766\/files\/IBSS_july-august_banner_1.webp?v=1722464358\" alt=\"July Banner - Click to view catalogues\">\n        <\/div>\n    <\/a>\n\n    <!-- Buttons Section -->\n    <div class=\"header-text\">\n        Click to go to\n    <\/div>\n    <div class=\"tabs-container\" id=\"tabsContainer\">\n        <div class=\"tabs\">\n            <div class=\"tab\" onclick=\"scrollToElement('CollectionSection-template--17214040834214__featured_collection_WcbkhV')\">Miskoka<\/div>\n            <div class=\"tab\" onclick=\"scrollToElement('ProductSection-template--17214040834214__featured_product_iEKVGM-8121459310758')\">Cutie Oils<\/div>\n            <div class=\"tab\" onclick=\"scrollToElement('shopify-section-template--17214040834214__featured_collection_CFEckt')\">Hemp & Body Co<\/div>\n            <div class=\"tab\" onclick=\"scrollToElement('ProductSection-template--17214040834214__featured_product_D9gQxr-8193215791270')\">Footlogix<\/div>\n            <div class=\"tab\" onclick=\"scrollToElement('ProductSection-template--17214040834214__featured_product_LAebir-7796175306918')\">Voesh<\/div>\n            <div class=\"tab\" onclick=\"scrollToElement('ProductSection-template--17214040834214__featured_product_tbP8BR-7777202340006')\">Bodyography<\/div>\n        <\/div>\n    <\/div>\n    <div class=\"scroll-arrow\">âžœ<\/div>\n\n    <script>\n    function scrollToElement(targetId) {\n        const offset = 130;\n        var element = document.getElementById(targetId);\n        if (element) {\n            const elementPosition = element.getBoundingClientRect().top + window.pageYOffset;\n            const offsetPosition = elementPosition - offset;\n            window.scrollTo({\n                top: offsetPosition,\n                behavior: \"smooth\"\n            });\n        } else {\n            console.error(\"Element not found: \" + targetId);\n        }\n    }\n\n    function adjustTabTextSizes() {\n        const tabs = document.querySelectorAll('.tab');\n        tabs.forEach(tab => {\n            const tabWidth = tab.offsetWidth;\n            const text = tab.textContent || tab.innerText;\n            let fontSize = parseInt(window.getComputedStyle(tab).fontSize);\n            const span = document.createElement('span');\n            span.style.cssText = 'font-size: ' + fontSize + 'px; visibility: hidden; position: absolute; white-space: nowrap;';\n            span.textContent = text;\n            document.body.appendChild(span);\n            while (span.offsetWidth > tabWidth - 20 && fontSize > 0) {\n                fontSize--;\n                span.style.fontSize = fontSize + 'px';\n            }\n            tab.style.fontSize = fontSize + 'px';\n            document.body.removeChild(span);\n        });\n    }\n\n    function updateScrollArrow() {\n        const container = document.getElementById('tabsContainer');\n        const scrollArrow = document.querySelector('.scroll-arrow');\n        \n        if (window.innerWidth < 768) {\n            const isScrollable = container.scrollWidth > container.clientWidth;\n            const isScrolledToEnd = container.scrollLeft >= container.scrollWidth - container.clientWidth - 5;\n            \n            scrollArrow.style.opacity = isScrollable && !isScrolledToEnd ? '1' : '0';\n            \n            \/\/ Update arrow position to stay centered with the tabs\n            const containerRect = container.getBoundingClientRect();\n            const arrowHeight = scrollArrow.offsetHeight;\n            scrollArrow.style.top = `${containerRect.top + (containerRect.height - arrowHeight) \/ 2}px`;\n        } else {\n            scrollArrow.style.opacity = '0';\n        }\n    }\n\n    const tabsContainer = document.getElementById('tabsContainer');\n    \n    \/\/ Debounce function to limit the rate at which a function can fire\n    function debounce(func, wait) {\n        let timeout;\n        return function executedFunction(...args) {\n            const later = () => {\n                clearTimeout(timeout);\n                func(...args);\n            };\n            clearTimeout(timeout);\n            timeout = setTimeout(later, wait);\n        };\n    }\n\n    \/\/ Debounced version of adjustTabTextSizes\n    const debouncedAdjustTabTextSizes = debounce(adjustTabTextSizes, 250);\n\n    tabsContainer.addEventListener('scroll', updateScrollArrow);\n    window.addEventListener('resize', () => {\n        updateScrollArrow();\n        debouncedAdjustTabTextSizes();\n    });\n    window.addEventListener('load', () => {\n        adjustTabTextSizes();\n        updateScrollArrow();\n    });\n    window.addEventListener('scroll', updateScrollArrow);\n    <\/script>\n<\/body>\n<\/html>","width":"100%","alignment":"center"}}},"block_order":["liquid_D7XdzH"],"settings":{"full_width":true,"space_around":false}},"featured_collection_WcbkhV":{"type":"featured-collection","settings":{"title":"NEW - Collagen Glass Masks","home_featured_products":"miskoka","count":5,"mobile_scrollable":true,"view_all":true,"divider":false}},"featured_product_iEKVGM":{"type":"featured-product","blocks":{"price_fdkKVm":{"type":"price","settings":{}},"description_dxyR9y":{"type":"description","settings":{"is_tab":false}},"variant_picker_qB9diA":{"type":"variant_picker","settings":{"variant_labels":true,"picker_type":"button","color_swatches":false}},"buy_buttons_e439qx":{"type":"buy_buttons","settings":{"show_dynamic_checkout":true,"surface_pickup_enable":true}}},"block_order":["price_fdkKVm","description_dxyR9y","variant_picker_qB9diA","buy_buttons_e439qx"],"settings":{"featured_product":"cutie-oil-goldie-6ml","divider":false,"sku_enable":false,"image_position":"left","image_container_size":"medium","product_image_size":"square","product_zoom_enable":true,"thumbnail_position":"beside","thumbnail_arrows":false,"mobile_layout":"partial","enable_video_looping":true,"product_video_style":"muted"}},"featured_collection_CFEckt":{"type":"featured-collection","settings":{"title":"Bath Bombs","home_featured_products":"hemp-body-co","count":10,"mobile_scrollable":true,"view_all":false,"divider":false}},"featured_product_D9gQxr":{"type":"featured-product","blocks":{"price_nKqMVR":{"type":"price","settings":{}},"description_W6HBtC":{"type":"description","settings":{"is_tab":false}},"variant_picker_FmAmf6":{"type":"variant_picker","settings":{"variant_labels":true,"picker_type":"button","color_swatches":false}},"buy_buttons_Lz6QRw":{"type":"buy_buttons","settings":{"show_dynamic_checkout":true,"surface_pic
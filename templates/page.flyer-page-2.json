en; position: absolute; white-space: nowrap;';\n            span.textContent = text;\n            document.body.appendChild(span);\n            while (span.offsetWidth > tabWidth - 20 && fontSize > 0) {\n                fontSize--;\n                span.style.fontSize = fontSize + 'px';\n            }\n            tab.style.fontSize = fontSize + 'px';\n            document.body.removeChild(span);\n        });\n    }\n\n    function updateScrollArrow() {\n        const container = document.getElementById('tabsContainer');\n        const scrollArrow = document.querySelector('.scroll-arrow');\n        \n        if (window.innerWidth < 768) {\n            const isScrollable = container.scrollWidth > container.clientWidth;\n            const isScrolledToEnd = container.scrollLeft >= container.scrollWidth - container.clientWidth - 5;\n            \n            scrollArrow.style.opacity = isScrollable && !isScrolledToEnd ? '1' : '0';\n            \n            \/\/ Align arrow with the tabs\n            const containerRect = container.getBoundingClientRect();\n            scrollArrow.style.top = `${containerRect.top + containerRect.height \/ 2 - 15}px`; \/\/ 15 is half the arrow height\n        } else {\n            scrollArrow.style.opacity = '0';\n        }\n    }\n\n    const tabsContainer = document.getElementById('tabsContainer');\n    \n    \/\/ Debounce function to limit the rate at which a function can fire\n    function debounce(func, wait) {\n        let timeout;\n        return function executedFunction(...args) {\n            const later = () => {\n                clearTimeout(timeout);\n                func(...args);\n            };\n            clearTimeout(timeout);\n            timeout = setTimeout(later, wait);\n        };\n    }\n\n    \/\/ Debounced version of adjustTabTextSizes\n    const debouncedAdjustTabTextSizes = debounce(adjustTabTextSizes, 250);\n\n    tabsContainer.addEventListener('scroll', updateScrollArrow);\n    window.addEventListener('resize', () => {\n        updateScrollArrow();\n        debouncedAdjustTabTextSizes();\n    });\n    window.addEventListener('load', () => {\n        adjustTabTextSizes();\n        updateScrollArrow();\n    });\n    window.addEventListener('scroll', updateScrollArrow);\n    <\/script>\n<\/body>\n<\/html>","width":"100%","alignment":"center"}}},"block_order":["liquid_4YWnie"],"settings":{"full_width":true,"space_around":false}},"featured_collection_gAkcrk":{"type":"featured-collection","settings":{"title":"Purchase 6 Litres, get 6 pumps free!","home_featured_products":"colorproof-shampoos-conditioners","count":25,"mobile_scrollable":true,"view_all":true,"divider":false}},"main":{"type":"main-page","disabled":true,"settings":{}},"featured_product_6cWWWW":{"type":"featured-product","blocks":{"price_iKzYhV":{"type":"price","settings":{}},"description_7aTcG9":{"type":"description","disabled":true,"settings":{"is_tab":false}},"variant_picker_TTF3BB":{"type":"variant_picker","settings":{"variant_labels":true,"picker_type":"button","color_swatches":false}},"buy_buttons_3hDrnr":{"type":"buy_buttons","settings":{"show_dynamic_checkout":true,"surface_pickup_enable":true}}},"block_order":["price_iKzYhV","description_7aTcG9","variant_picker_TTF3BB","buy_buttons_3hDrnr"],"settings":{"featured_product":"cp-biorepair-thicken-kit","divider":false,"sku_enable":false,"image_position":"left","image_container_size":"medium","product_image_size":"square","product_zoom_enable":true,"thumbnail_position":"beside","thumbnail_arrows":false,"mobile_layout":"partial","enable_video_looping":true,"product_video_style":"muted"}},"hc_carousel_nREbpa":{"type":"hc-carousel","blocks":{"product_heiQjL":{"type":"product","settings":{"hc_product":"re-bond-miracle-bond-building-mask-pro-size-236ml-8oz"}},"product_hdcDTm":{"type":"product","settings":{"hc_product":"re-bond-mircle-bond-building-mask-pro-size-offer"}}},"block_order":["product_heiQjL","product_hdcDTm"],"settings":{"hc_autoplay":true,"hc_speed":"8
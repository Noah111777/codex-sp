ight: 0;\n    background-color: rgba(0, 0, 0, 0.7);\n    color: white;\n    padding: 10px;\n    margin: 0;\n    font-size: 14px;\n    text-align: center;\n  }\n\n  .se-collection-link {\n    text-decoration: none;\n    color: inherit;\n    display: block;\n    height: 100%;\n  }\n\n  .se-nav-arrow {\n    display: none;\n    position: absolute;\n    top: 50%;\n    transform: translateY(-50%);\n    background-color: rgba(0, 0, 0, 0.5);\n    color: white;\n    border: none;\n    font-size: 24px;\n    padding: 10px 15px;\n    cursor: pointer;\n    z-index: 10;\n    transition: background-color 0.3s ease;\n  }\n\n  .se-nav-arrow:hover {\n    background-color: rgba(0, 0, 0, 0.8);\n  }\n\n  .se-prev-arrow {\n    left: 5px;\n  }\n\n  .se-next-arrow {\n    right: 5px;\n  }\n\n  .se-scroll-indicator-container {\n    width: 100%;\n    height: 4px;\n    background-color: #e0e0e0;\n    position: relative;\n    margin-top: 10px;\n  }\n\n  .se-scroll-indicator {\n    height: 100%;\n    background-color: #333;\n    position: absolute;\n    left: 0;\n    top: 0;\n    width: 0;\n    transition: width 0.3s ease;\n  }\n\n  \/* Tablet and Desktop styles *\/\n  @media (min-width: 768px) {\n    .se-collection-carousel-container {\n      padding: 0 40px;\n    }\n\n    .se-collection-item {\n      width: calc(20% - 12px);\n    }\n\n    .se-collection-title {\n      font-size: 16px;\n    }\n\n    .se-nav-arrow {\n      display: block;\n    }\n\n    .se-prev-arrow {\n      left: 10px;\n    }\n\n    .se-next-arrow {\n      right: 10px;\n    }\n  }\n<\/style>\n\n<script>\n  document.addEventListener('DOMContentLoaded', function() {\n    const carousel = document.querySelector('.se-collection-carousel');\n    const prevArrow = document.querySelector('.se-prev-arrow');\n    const nextArrow = document.querySelector('.se-next-arrow');\n    const items = carousel.querySelectorAll('.se-collection-item');\n    const scrollIndicator = document.querySelector('.se-scroll-indicator');\n    \n    function getScrollAmount() {\n      const itemWidth = items[0].offsetWidth;\n      const itemMargin = parseInt(window.getComputedStyle(items[0]).marginRight);\n      return (itemWidth + itemMargin) * 2;\n    }\n\n    function updateArrowVisibility() {\n      if (window.innerWidth >= 768) {\n        prevArrow.style.display = carousel.scrollLeft <= 0 ? 'none' : 'block';\n        nextArrow.style.display = carousel.scrollLeft >= carousel.scrollWidth - carousel.clientWidth ? 'none' : 'block';\n      } else {\n        prevArrow.style.display = 'none';\n        nextArrow.style.display = 'none';\n      }\n    }\n\n    function updateScrollIndicator() {\n      const scrollPercentage = (carousel.scrollLeft \/ (carousel.scrollWidth - carousel.clientWidth)) * 100;\n      scrollIndicator.style.width = `${scrollPercentage}%`;\n    }\n\n    prevArrow.addEventListener('click', () => {\n      carousel.scrollBy({left: -getScrollAmount(), behavior: 'smooth'});\n    });\n\n    nextArrow.addEventListener('click', () => {\n      carousel.scrollBy({left: getScrollAmount(), behavior: 'smooth'});\n    });\n\n    carousel.addEventListener('scroll', () => {\n      updateArrowVisibility();\n      updateScrollIndicator();\n    });\n    \n    window.addEventListener('resize', () => {\n      updateArrowVisibility();\n      updateScrollIndicator();\n    });\n\n    \/\/ Initial updates\n    updateArrowVisibility();\n    updateScrollIndicator();\n  });\n<\/script>","width":"100%","alignment":"center"}}},"block_order":["template--16370952044710__d5f45208-a933-490b-a856-084922965127-liquid-1"],"settings":{"full_width":true,"space_around":false}},"advanced_content_WVrcDX":{"type":"advanced-content","blocks":{"liquid_DiMtxx":{"type":"liquid","settings":{"code":"<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n<meta charset=\"UTF-8\">\n<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n<title>Unique Grid Layout<\/title>\n<link rel=\"stylesheet\" href=\"styles.css\"> <!-- Link to your CSS file